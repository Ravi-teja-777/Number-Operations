public class Main {
    public static void main(String[] args) {
      
      System.out.println("the maximum value is : " + Math.max(7,77));
      System.out.println("the minimum value is : " + Math.min(77,7));
      System.out.println("the square root value is : " + Math.sqrt(49));
      System.out.println("the absolute value is : " + Math.abs(-7));
      System.out.println("the maximum value is : " + Math.random()*7);
  }
}
